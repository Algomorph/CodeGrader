{
  "name": "code-grader",
  "version": "1.0.3",
  "description": "code style grading aid",
  "keywords": [
    "java",
    "js",
    "code",
    "grader"
  ],
  "homepage": "codegrader.net",
  "bugs": {
    "url": "https://github.com/Algomorph/CodeGrader/issues",
    "email": "authors@codegrader.net"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Gregory Kramida",
  "contributors": [
    "William Siew"
  ],
  "maintainers": [
    "Gregory Kramida",
    "Matthew Simmons",
    "Andrej Rasevic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Algomorph/CodeGrader.git"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.json5?$": "json5-jest"
    },
    "collectCoverageFrom": [
      "modules/*.js",
      "code_analysis/*.js"
    ],
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "dependencies": {
    "browserify": "^17.0.0",
    "include": "^1.1.0",
    "jest": "^27.0.6",
    "js-beautify": "^1.14.0",
    "json5": "^2.2.1",
    "multiline": "^2.0.0",
    "json5-jest": "^1.0.1"
  },
  "devDependencies": {},
  "scripts": {
    "build": "browserify options.js options_controller.js -o options_compiled.js"
  }
}
